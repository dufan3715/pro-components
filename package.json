{
  "name": "pro-components",
  "private": true,
  "version": "0.0.0",
  "keywords": [],
  "author": "",
  "scripts": {
    "dev": "pnpm run examples:dev",
    "examples:dev": "pnpm -F examples dev",
    "prepare": "husky install",
    "lint:eslint": "eslint --cache --max-warnings 0 \"src/**/*.{vue,js,ts,tsx}\" --fix",
    "lint:stylelint": "stylelint --cache --fix \"src/**/*.{vue,css,scss,postcss,less}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint": "pnpm lint:eslint && pnpm lint:stylelint && pnpm lint:prettier",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "clean": "rm -rf node_modules **/*/node_modules"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/lodash-es": "^4.17.9",
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.9.2",
    "stylelint": "^15",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-less": "^1.0.6",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^3.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.5.3",
    "vue-tsc": "^1.6.5"
  }
}
