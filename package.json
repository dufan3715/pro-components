    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,css,scss,less}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:prettier": "prettier --write  \"**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint": "pnpm lint:eslint && pnpm lint:stylelint && pnpm lint:prettier",
    "lint:lint-staged": "lint-staged -c lint-staged.config.mjs",
    "clean": "rm -rf node_modules **/*/node_modules"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/css": "^0.6.0",
