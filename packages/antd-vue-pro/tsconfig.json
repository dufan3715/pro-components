{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "types": ["ant-design-vue/typings/global"],
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "./es",
    "emitDeclarationOnly": false,
    "isolatedModules": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "outDir": "./es",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "vue-types": ["node_modules/vue-types"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "es"]
}
